{
  "name": "twitter-clone",
  "version": "0.0.0",
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve  --host 0.0.0.0",
    "build": "ng build",
    "test": "npx ng test --code-coverage",
    "test-cli": "npx ng test --karma-config=src/karma/karma-cli.conf.js --code-coverage",
    "lint": "npx ng lint",
    "deploy:gh-pages": "npx ng build --configuration production --output-path docs  --base-href https://mcknasty.github.io/twitter-angular-clone.github.io/",
    "circleci:local": "circleci local execute -v $(pwd):/tmp/repo --job local-build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.1.2",
    "@angular/common": "~14.1.2",
    "@angular/core": "~14.1.2",
    "@angular/forms": "~14.1.2",
    "@angular/language-service": "~14.1.3",
    "@angular/platform-browser": "~14.1.3",
    "@angular/platform-browser-dynamic": "~14.1.2",
    "@angular/router": "~14.1.2",
    "angular-in-memory-web-api": "^0.14.0",
    "moment": "^2.29.4",
    "ngx-moment": "^6.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.1.2",
    "@angular-devkit/schematics": "^14.1.2",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
